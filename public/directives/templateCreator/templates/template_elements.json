    }, {
        "name": "font",
        "type": "text"
    }]
}, {
    "name": "Button",
    "html": "<div class='snOutDiv'><div class='snDiv'><div class='snItem'><input type='button' enabled='false' class='btn btn-default container_item' value='{{options[0].value}}'  sn-item='true' sn-item-click='onItemClick($id)' id='Button'></div><div class='snText'>Button</div></div></div>",
